{
  "title": "Cluster Overview",
  "_base": "dashboard",
  "annotations": {
    "list": [
      {
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": false,
        "iconColor": "#ffffff",
        "limit": 500,
        "matchAny": true,
        "name": "Topology",
        "showIn": 0,
        "tags": [
          "topology"
        ],
        "type": "tags"
      },
      {
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": false,
        "iconColor": "#115fd4",
        "limit": 500,
        "matchAny": true,
        "name": "Buckets",
        "showIn": 0,
        "tags": [
          "buckets"
        ],
        "type": "tags"
      }
    ]
  },
  "_panels": [
    {
      "title": "sys_cpu_host_utilization_rate",
      "_base": "panel",
      "_targets": [
        {
          "datasource": "{data-source-name}",
          "expr": "sys_cpu_host_utilization_rate",
          "legendFormat": "{data-source-name} sys_cpu_host_utilization_rate",
          "_base": "target"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 1
          },
          "unit": "percent"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        }
      },
      "type": "timeseries"
    },
    {
      "title": "sys_mem_limit and used",
      "_base": "panel",
      "_targets": [
        {
          "datasource": "{data-source-name}",
          "expr": "sys_mem_limit",
          "legendFormat": "{data-source-name} {{name}}",
          "_base": "target"
        },
        {
          "datasource": "{data-source-name}",
          "expr": "sys_mem_actual_used",
          "legendFormat": "{data-source-name} {{name}}",
          "_base": "target"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 1
          },
          "unit": "bytes"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        }
      },
      "type": "timeseries"
    },
    {
      "title": "sys_mem_free",
      "_base": "panel",
      "_targets": [
        {
          "datasource": "{data-source-name}",
          "expr": "sys_mem_free",
          "legendFormat": "{data-source-name} sys_mem_free",
          "_base": "target"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 1
          },
          "unit": "bytes"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        }
      },
      "type": "timeseries"
    },
    {
      "title": "total kv_ops",
      "_base": "panel",
      "_targets": [
        {
          "datasource": "{data-source-name}",
          "expr": "sum(irate(kv_ops[1m]))",
          "legendFormat": "{data-source-name} sum(irate(kv_ops[1m]))",
          "_base": "target"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 1
          },
          "unit": "short"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        }
      },
      "type": "timeseries"
    },
    {
      "title": "kv gets and mutations",
      "_base": "panel",
      "_targets": [
        {
          "datasource": "{data-source-name}",
          "expr": "sum(irate(kv_ops{op=\"get\"}[1m]))",
          "legendFormat": "{data-source-name} sum(irate(kv_ops{op=get}[1m]))",
          "_base": "target"
        },
        {
          "datasource": "{data-source-name}",
          "expr": "sum(irate(kv_ops{op=~\"set|set_meta|set_ret_meta|cas|decr|del_meta|del_ret_meta|incr|delete\"}[1m]))",
          "legendFormat": "{data-source-name} sum(irate(kv_ops{op=~set|set_meta|cas|delete|...}[1m]))",
          "_base": "target"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 1
          },
          "unit": "short"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        }
      },
      "type": "timeseries"
    },
    {
      "title": "sum(kv_mem_used_bytes)",
      "_base": "panel",
      "_targets": [
        {
          "datasource": "{data-source-name}",
          "expr": "sum(kv_mem_used_bytes)",
          "legendFormat": "{data-source-name} sum(kv_mem_used_bytes)",
          "_base": "target"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 1
          },
          "unit": "bytes"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        }
      },
      "type": "timeseries"
    },
    {
      "title": "irate(n1ql_requests[1m])",
      "_base": "panel",
      "_targets": [
        {
          "datasource": "{data-source-name}",
          "expr": "irate(n1ql_requests[1m])",
          "legendFormat": "{data-source-name} {{name}}",
          "_base": "target"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 1
          },
          "unit": "short"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        }
      },
      "type": "timeseries"
    },
    {
      "title": "n1ql selects, updates & inserts",
      "_base": "panel",
      "_targets": [
        {
          "datasource": "{data-source-name}",
          "expr": "irate(n1ql_selects[1m])",
          "legendFormat": "{data-source-name} {{name}}",
          "_base": "target"
        },
        {
          "datasource": "{data-source-name}",
          "expr": "irate(n1ql_updates[1m])",
          "legendFormat": "{data-source-name} {{name}}",
          "_base": "target"
        },
        {
          "datasource": "{data-source-name}",
          "expr": "irate(n1ql_inserts[1m])",
          "legendFormat": "{data-source-name} {{name}}",
          "_base": "target"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 1
          },
          "unit": "percent"
        }
      },
      "options": {
        "tooltip": {
          "mode": "short"
        }
      },
      "type": "timeseries"
    },
    {
      "title": "index items",
      "_base": "panel",
      "_targets": [
        {
          "datasource": "{data-source-name}",
          "expr": "sum(index_items_count)",
          "legendFormat": "{data-source-name} sum(index_items_count)",
          "_base": "target"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 1
          },
          "unit": "short"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        }
      },
      "type": "timeseries"
    },
    {
      "title": "index scans / second",
      "_base": "panel",
      "_targets": [
        {
          "datasource": "{data-source-name}",
          "expr": "sum(irate(index_num_rows_scanned[1m]))",
          "legendFormat": "{data-source-name} sum(irate(index_num_rows_scanned[1m]))",
          "_base": "target"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 1
          },
          "unit": "short"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        }
      },
      "type": "timeseries"
    },
    {
      "title": "sum(index_memory_used)",
      "_base": "panel",
      "_targets": [
        {
          "datasource": "{data-source-name}",
          "expr": "sum(index_memory_used)",
          "legendFormat": "{data-source-name} sum(index_memory_used)",
          "_base": "target"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 1
          },
          "unit": "bytes"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        }
      },
      "type": "timeseries"
    },
    {
      "title": "Disk usage summed over buckets",
      "_base": "panel",
      "_targets": [
        {
          "datasource": "{data-source-name}",
          "expr": "sum(couch_docs_actual_disk_size)",
          "legendFormat": "{data-source-name} sum(couch_docs_actual_disk_size)",
          "_base": "target"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 1
          },
          "unit": "bytes"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        }
      },
      "type": "timeseries"
    }
  ]
}

