{
  "_base": "dashboard",
  "title": "Query Service Dashboard",
  "_panels": [
    {
      "title": "CPU Utilization",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "stack": true,
      "_targets": [
        {
          "expr": "sysproc_cpu_utilization{proc=\"cbq-engine\"}",
          "legendFormat": "CPU Utilization",
          "_base": "target"
        }
      ]
    },
    {
      "title": "Memory Usage",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "stack": true,
      "_targets": [
        {
          "expr": "sysproc_mem_resident{proc=\"cbq-engine\"}",
          "legendFormat": "Memory Usage",
          "_base": "target"
        }
      ]
    },
    {
      "title": "N1QL Operations",
      "_base": "panel",
      "stack": true,
      "_targets": [
        {
          "datasource": "{data-source-name}",
          "expr": "irate(n1ql_selects[1m])",
          "legendFormat": "Select Ops",
          "_base": "target"
        },
        {
          "datasource": "{data-source-name}",
          "expr": "irate(n1ql_updates[1m])",
          "legendFormat": "Update Ops",
          "_base": "target"
        },
        {
          "datasource": "{data-source-name}",
          "expr": "irate(n1ql_inserts[1m])",
          "legendFormat": "Insert Ops",
          "_base": "target"
        },
        {
          "datasource": "{data-source-name}",
          "expr": "irate(n1ql_deletes[1m])",
          "legendFormat": "Delete Ops",
          "_base": "target"
        }
      ]
    }
  ]
}