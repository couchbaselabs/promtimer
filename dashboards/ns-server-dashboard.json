{
  "title": "NS Server Dashboard",
  "annotations": {
    "list": [
      {
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": false,
        "iconColor": "#ffffff",
        "limit": 500,
        "matchAny": true,
        "name": "Topology",
        "showIn": 0,
        "tags": [
          "topology"
        ],
        "type": "tags"
      },
      {
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": false,
        "iconColor": "#115fd4",
        "limit": 500,
        "matchAny": true,
        "name": "Buckets",
        "showIn": 0,
        "tags": [
          "buckets"
        ],
        "type": "tags"
      }
    ]
  },
  "_base": "dashboard",
  "_panels": [
    {
      "title": "CPU",
      "_base": "row"
    },
    {
      "title": "sys_cpu_host_utilization_rate",
      "_base": "panel",
      "_targets": [
        {
          "datasource": "{data-source-name}",
          "expr": "sys_cpu_host_utilization_rate",
          "legendFormat": "{data-source-name} {{name}}",
          "_base": "target"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 1
          },
          "unit": "percent"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        }
      },
      "type": "timeseries"
    },
    {
      "title": "total cpu & ns-server cpu utilization",
      "_base": "panel",
      "_targets": [
        {
          "datasource": "{data-source-name}",
          "expr": "sys_cpu_host_utilization_rate * ignoring(name)sys_cpu_cores_available",
          "legendFormat": "{data-source-name} sys_cpu_host_utilization_rate (in cores)",
          "_base": "target"
        },
        {
          "datasource": "{data-source-name}",
          "expr": "sysproc_cpu_utilization{proc=\"ns_server\"}",
          "legendFormat": "{data-source-name} {{name}} {{proc}}",
          "_base": "target"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 1
          },
          "unit": "percent"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        }
      },
      "type": "timeseries"
    },
