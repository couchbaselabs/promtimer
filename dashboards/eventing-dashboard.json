{
  "_base": "dashboard",
  "title": "Eventing Service Dashboard",
  "_panels": [
    {
      "title": "CPU Utilization",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "stack": true,
      "_targets": [
        {
          "expr": "sysproc_cpu_utilization{proc=\"eventing-produc\"}",
          "legendFormat": "CPU Utilization",
          "_base": "target"
        }
      ]
    },
    {
      "title": "Memory Usage",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "stack": true,
      "_targets": [
        {
          "expr": "sysproc_mem_resident{proc=\"eventing-produc\"}",
          "legendFormat": "Memory Usage",
          "_base": "target"
        }
      ]
    },
    {
      "title": "Number of Failures",
      "type": "stat",
      "_base": "panel",
      "_targets": [
        {
          "datasource": "{data-source-name}",
          "expr": "eventing_failed_count",
          "legendFormat": "Failures",
          "_base": "target"
        },
        {
          "datasource": "{data-source-name}",
          "expr": "eventing_timeout_count",
          "legendFormat": "Timeouts",
          "_base": "target"
        }
      ]
    },
    {
      "title": "Processed Count",
      "type": "stat",
      "_base": "panel",
      "_targets": [
        {
          "datasource": "{data-source-name}",
          "expr": "eventing_processed_count",
          "legendFormat": "Processed Count",
          "_base": "target"
        }
      ]
    }
  ]
}