{
  "_base": "dashboard",
  "title": "Node Overview",
  "templating": {
    "list": [
      {
        "hide": 0,
        "includeAll": false,
        "label": null,
        "multi": false,
        "name": "node",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "type": "datasource"
      }
    ]
  },
  "_panels": [
    {
      "title": "sys_cpu_utilization_rate",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "sys_cpu_utilization_rate",
          "legendFormat": "",
          "_base": "target"
        }
      ]
    },
    {
      "title": "sysproc_cpu_utilization",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "sysproc_cpu_utilization",
          "legendFormat": "{{proc}}",
          "_base": "target"
        }
      ]
    },
    {
      "title": "sysproc_mem_resident",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "sysproc_mem_resident",
          "legendFormat": "{{proc}}",
          "_base": "target"
        }
      ]
    },
    {
      "title": "kv_curr_items",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_curr_items",
          "legendFormat": "kv_curr_items {{bucket}}",
          "_base": "target"
        }
      ]
    },
    {
      "title": "sum(irate(kv_ops[1m]))",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "sum(irate(kv_ops[1m]))",
          "legendFormat": "sum(irate(kv_ops[1m]))",
          "_base": "target"
        }
      ]
    },
    {
      "title": "sum by (bucket) (irate(kv_ops[1m]))",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "sum by (bucket) (irate(kv_ops[1m]))",
          "legendFormat": "{{bucket}}: sum(irate(kv_ops[1m]))",
          "_base": "target"
        }
      ]
    },
    {
      "title": "sys_rest_requests rate",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "irate(sys_rest_requests[1m])",
          "legendFormat": "irate(sys_rest_requests[1m])",
          "_base": "target"
        }
      ]
    }
  ]
}
