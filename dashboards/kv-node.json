{
  "_base": "dashboard",
  "title": "KV View - Node",
  "annotations": {
    "list": [
      {
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": false,
        "iconColor": "#F2495C",
        "limit": 100,
        "matchAny": true,
        "name": "Failures",
        "showIn": 0,
        "tags": [
          "failure"
        ],
        "type": "tags"
      },
      {
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": false,
        "iconColor": "#FF9830",
        "limit": 100,
        "matchAny": true,
        "name": "Warnings",
        "showIn": 0,
        "tags": [
          "warning"
        ],
        "type": "tags"
      },
      {
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": false,
        "iconColor": "#73BF69",
        "limit": 100,
        "matchAny": true,
        "name": "Successes",
        "showIn": 0,
        "tags": [
          "success"
        ],
        "type": "tags"
      },
      {
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": false,
        "iconColor": "rgb(212, 212, 212)",
        "limit": 100,
        "matchAny": true,
        "name": "Info",
        "showIn": 0,
        "tags": [
          "info"
        ],
        "type": "tags"
      }
    ]
  },
  "templating": {
    "list": [
      {
        "hide": 0,
        "includeAll": false,
        "label": null,
        "multi": false,
        "name": "node",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "type": "datasource"
      }
    ]
  },
  "_panels": [
    {
      "title": "Connections",
      "_base": "row"
    },
    {
      "title": "Connections Ever Opened",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_total_connections",
          "legendFormat": "Connections",
          "_base": "target"
        }
      ]
    },
    {
      "title": "Current Connections",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_curr_connections",
          "legendFormat": "Current",
          "_base": "target"
        },
        {
          "expr": "kv_daemon_connections",
          "legendFormat": "Daemon",
          "_base": "target"
        },
        {
          "expr": "kv_system_connections",
          "legendFormat": "System",
          "_base": "target"
        }
      ],
      "stack": true
    },
    {
      "title": "Number of Auth Errors",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_auth_errors",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ],
      "description": "Number of auth errors"
    },
    {
      "title": "DCP",
      "_base": "row"
    },
    {
      "title": "Backoff Events",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_dcp_backoff",
          "legendFormat": "{{bucket}} ({{connection_type}})",
          "_base": "target"
        }
      ],
      "description": "Total number of backoff events"
    },
    {
      "title": "Idle Timeout",
      "type": "gauge",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_ep_dcp_idle_timeout",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ],
      "description": "DCP idle timeout value",
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        }
      }
    },
    {
      "title": "Producer Count",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_dcp_producer_count",
          "legendFormat": "{{bucket}} ({{connection_type}})",
          "_base": "target"
        }
      ],
      "description": "Number of DCP Producers"
    },
    {
      "title": "Remaining Items",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_dcp_items_remaining",
          "legendFormat": "{{bucket}} ({{connection_type}})",
          "_base": "target"
        }
      ],
      "description": "Total items remaining to be sent"
    },
    {
      "title": "Sent Items",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "rate(kv_dcp_items_sent[1m])",
          "legendFormat": "{{bucket}} ({{connection_type}})",
          "_base": "target"
        }
      ],
      "description": "Total items sent"
    },
    {
      "title": "Total Connections",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_dcp_connection_count",
          "legendFormat": "{{bucket}} ({{connection_type}})",
          "_base": "target"
        }
      ],
      "description": "Connection count"
    },
    {
      "title": "Total Data Size",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_dcp_total_data_size_bytes",
          "legendFormat": "{{bucket}} ({{connection_type}})",
          "_base": "target"
        }
      ]
    },
    {
      "title": "Data",
      "_base": "row"
    },
    {
      "title": "Data Read Failures",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_ep_data_read_failed",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ]
    },
    {
      "title": "Data Write Failures",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_ep_data_write_failed",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ]
    },
    {
      "title": "Doc Size",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "couch_docs_actual_disk_size",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ]
    },
    {
      "title": "Number of Docs (inc. Replicas)",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_curr_items_tot",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ]
    },
    {
      "title": "Percentage Memory Resident",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "sum(kv_vb_perc_mem_resident_ratio) by (bucket)",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ]
    },
    {
      "title": "Rate of Deletes",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "rate(kv_ep_total_del_items[1m])",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ],
      "description": "The number of deleted documents, converted into tombstones, every minute"
    },
    {
      "title": "Rate of Doc Expirations",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "sum(rate(kv_vb_expired[1m])) by (bucket)",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ]
    },
    {
      "title": "Rollback Item Count",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_rollback_item_count",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ],
      "description": "Number of items rolled back?"
    },
    {
      "title": "Size of Key",
      "type": "stat",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_ep_bfilter_key_count",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ],
      "description": "Minimum key count that bloom filter will accomodate"
    },
    {
      "title": "Disk",
      "_base": "row"
    },
    {
      "title": "Data Size",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "couch_docs_actual_disk_size",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ]
    },
    {
      "title": "Disk Queue Items",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_ep_diskqueue_items",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ],
      "description": "Total number of items in disk queue"
    },
    {
      "title": "Disk Queue Memory Usage",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_ep_diskqueue_memory_bytes",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ],
      "description": "Total memory used by disk queue, in bytes"
    },
    {
      "title": "New and Persisted Items",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "sum(kv_ep_total_new_items) by (bucket) + sum(kv_ep_total_persisted) by (bucket)",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ]
    },
    {
      "title": "Written Bytes",
      "datasource": "{data-source-name}",
      "gridPos": {
        "w": 6
      },
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_written_bytes",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ]
    },
    {
      "title": "Read Bytes",
      "datasource": "{data-source-name}",
      "gridPos": {
        "w": 6
      },
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_read_bytes",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ]
    },
    {
      "title": "General",
      "_base": "row"
    },
    {
      "title": "KV CPU Utilization",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "sysproc_cpu_utilization{proc=\"memcached\"}",
          "legendFormat": "Percentage",
          "_base": "target"
        }
      ],
      "description": "KV service CPU Utilization of all cores summed up, in percent\n\nsysproc_cpu_utilization{proc=\"memcached\"}"
    },
    {
      "title": "KV Uptime",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_uptime_seconds",
          "legendFormat": "Seconds",
          "_base": "target"
        }
      ],
      "description": "KV service uptime in seconds"
    },
    {
      "title": "Memory Usage",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "sysproc_mem_resident{proc=\"memcached\"}",
          "legendFormat": "Bytes",
          "_base": "target"
        }
      ],
      "description": "Memory usage by the KV service, in bytes\n\nsysproc_mem_resident{proc=\"memcached\"}"
    },
    {
      "title": "Memory",
      "_base": "row"
    },
    {
      "title": "Checkpoint Memory Size",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_ep_checkpoint_memory_bytes",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ],
      "description": "Memory of items in all checkpoints, in bytes"
    },
    {
      "title": "Defragmenter Enabled",
      "type": "bargauge",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_ep_defragmenter_enabled",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ],
      "description": "Whether the defragmenter is enabled or not",
      "fieldConfig": {
        "defaults": {
          "max": 1,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        }
      }
    },
    {
      "title": "Defragmenter Interval",
      "type": "gauge",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_ep_defragmenter_interval",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ],
      "description": "How often defragmenter task should run, in seconds",
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        }
      }
    },
    {
      "title": "Metadata Overhead",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "sum(kv_vb_meta_data_memory_bytes) by (bucket)",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ]
    },
    {
      "title": "OOM Errors Total",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_ep_oom_errors",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ]
    },
    {
      "title": "OOM Errors at Warmup",
      "datasource": "{data-source-name}",
      "_base": "panel",
      "_targets": [
        {
          "expr": "kv_ep_warmup_oom",
          "legendFormat": "{{bucket}}",
          "_base": "target"
        }
      ]
    }
  ]
}